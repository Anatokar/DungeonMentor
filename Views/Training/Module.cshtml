@{
    ViewData["Title"] = "Модуль обучения";
    var moduleName = ViewData["ModuleName"] as string;
}

<h2>@moduleName</h2>

<p>Описание и материалы модуля "@moduleName"...</p>

<form asp-action="Complete" method="post">
    <input type="hidden" name="moduleName" value="@moduleName" />
    <input type="hidden" name="score" value="100" /> <!-- временный балл -->
    <button type="submit" class="btn btn-success">Завершить модуль</button>
</form>
